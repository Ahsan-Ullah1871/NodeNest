{
    "name": "ah-starter-kit",
    "version": "1.0.0",
    "description": "A Node.js and Express starter kit with TypeScript.",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "test": "test",
        "start": "ts-node-dev --respawn --transpile-only src/server.ts",
        "dev": "set NODE_ENV=development && ts-node-dev --respawn --transpile-only src/server.ts",
        "prod": "set NODE_ENV=production && ts-node-dev --respawn --transpile-only src/server.ts",
        "lint:check": "eslint --ignore-path .eslintignore --ext .js,.ts .",
        "lint:fix": "eslint --fix",
        "prettier:check": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
        "lint-prettier": "yarn lint:check && yarn prettier:check",
        "build": "tsc",
        "prepare": "npm run build",
        "prepublishOnly": "npm run lint:check",
        "preversion": "npm run lint:check",
        "version": "npm run prettier:check && git add -A src",
        "postversion": "git push && git push --tags"
    },
    "bin": {
        "create-ah-starter": "./dist/index.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Ahsan-Ullah1871/ah-starter-kit.git"
    },
    "keywords": [
        "Node.js",
        "Express",
        "TypeScript",
        "starter",
        "template",
        "create-starter",
        "boilerplate"
    ],
    "author": "Ahsanullah",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Ahsan-Ullah1871/ah-starter-kit/issues"
    },
    "homepage": "https://github.com/Ahsan-Ullah1871/ah-starter-kit#readme",
    "dependencies": {
        "@typescript-eslint/eslint-plugin": "^6.7.5",
        "@typescript-eslint/parser": "^6.7.5",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "http-status": "^1.7.0",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/cookie-parser": "^1.4.4",
        "@types/cors": "^2.8.14",
        "@types/express": "^4.17.18",
        "@types/jsonwebtoken": "^9.0.3",
        "eslint": "^8.47.0",
        "prettier": "^2.8.8",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.2.2"
    },
    "files": [
        "dist",
        "src",
        "README.md"
    ],
    "engines": {
        "node": ">=14.0.0"
    }
}